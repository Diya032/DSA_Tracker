////////////// Memory Limit Exceed ////////////

class Solution {
public:
    int findKthPositive(vector<int>& arr, int k) {
        vector<int> res;
        int cnt=1,i=0;
        
        while(arr[i]){
            if(arr[i]!=cnt){
                res.push_back(cnt);
                cnt++;
            }
            else{
                i++;
                cnt++;
            }
        }
        return res[k-1];
        
    }
};

